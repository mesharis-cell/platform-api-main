{
    "name": "drizzle",
    "version": "1.0.0",
    "packageManager": "bun@1.2.22",
    "main": "index.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only --watch ./src/app ./src/server.ts",
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write .",
        "lint": "eslint \"src/**/*.ts\"",
        "predeploy": "bun run format && bun run lint && bun run typecheck",
        "test": "echo \"Error: no test specified\" && exit 1",
        "db:seed": "tsx src/db/scripts/seed-safe.ts",
        "db:rebuild": "tsx src/db/scripts/rebuild-schema.ts",
        "db:reset": "bun run db:rebuild && bun run db:seed"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.958.0",
        "@aws-sdk/s3-request-presigner": "^3.958.0",
        "@pulumi/aws": "^7.16.0",
        "@pulumi/pulumi": "^3.216.0",
        "bcrypt": "^6.0.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dayjs": "^1.11.19",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.45.1",
        "exceljs": "^4.4.0",
        "express": "^5.2.1",
        "http-status": "^2.1.0",
        "jsonwebtoken": "^9.0.3",
        "ms": "^2.1.3",
        "multer": "^2.0.2",
        "node-cron": "^4.2.1",
        "nodemailer": "^7.0.12",
        "papaparse": "^5.5.3",
        "pdfkit": "^0.17.2",
        "pg": "^8.16.3",
        "qrcode": "^1.5.4",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/express-serve-static-core": "^5.1.1",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/ms": "^2.1.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^25.0.2",
        "@types/node-cron": "^3.0.11",
        "@types/nodemailer": "^7.0.4",
        "@types/papaparse": "^5.5.2",
        "@types/pdfkit": "^0.17.4",
        "@types/pg": "^8.16.0",
        "@types/qrcode": "^1.5.6",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^8.53.1",
        "@typescript-eslint/parser": "^8.53.1",
        "drizzle-kit": "^0.31.8",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.1.0",
        "prettier": "^3.7.4",
        "ts-node-dev": "^2.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "bun": ">=1.2.0",
        "node": ">=20"
    }
